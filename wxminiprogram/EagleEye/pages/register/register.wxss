.container { 
 
  height: 100%; 
 
  display: flex; 
 
  flex-direction: column; 
 
  padding: 0; 
 
  box-sizing: border-box; 
 
  background-color: #F2F2F2
 
 } 
 
 .register-icon{ 
 
  flex: none; 
 
 } 
 
 .register-img{ 
 
  margin-top: 20px;

  margin-left: 0px;
 
  width: 80px; 
 
  height: 80px
 
 } 
 
 .registerBtnView { 
 
  width: 100%; 
 
  height: auto; 
 
  background-color: #ffffff; 
 
  margin-top: 0px; 
 
  margin-bottom: 0px; 
 
  padding-bottom: 0px; 
 
 } 
 
 .returnBtnView { 
 
  width: 100%; 
 
  height: auto; 
 
  background-color: #ffffff; 
 
  margin-top: 0px; 
 
  margin-bottom: 0px; 
 
  padding-bottom: 0px; 
  
  border:none;
 
 }


 
 .registerBtn::after{
   border:none;
 }
 
   
 
 .registerBtn { 
 
  width: 80%; 

  color:#ffffff;
 
  background-color:#515151;
 
  margin-top: 15px; 
 
 }
 
 .returnBtn { 
 
  width: 50%; 
 
  height:40px;
 
  color:#515151;
 
  background-color: #ffffff;
 
  margin-top: 0px; 
 
  border:none;
 
 }

 

 .weui-vcode-img{width:108px}